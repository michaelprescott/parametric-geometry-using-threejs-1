    "statusBarItem.hoverBackground": "#039edb",
    "titleBar.activeBackground": "#035f83",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#035f8399",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#039edb",
